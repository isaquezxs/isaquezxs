Tab4:AddButton({
    Name = "Puxar Parts",
    Description = "Para usar, chegue perto do Player Selecionado",
    Callback = function()
        game.StarterGui:SetCore("SendNotification", {
            Title = "Puxar Parts",
            Text = "Fun√ß√£o em desenvolvimento",
            Duration = 5
        })
    end
})

-- Tab5: Sistema de √Åudio
local Tab5 = Window:MakeTab({"Audio", "radio"})

local loopAtivo = false
local InputID = ""

Tab5:AddTextBox({
    Name = "ID do √Åudio",
    Description = "Digite o ID do som",
    Default = "",
    PlaceholderText = "Exemplo: 6832470734",
    ClearTextOnFocus = true,
    Callback = function(text)
        InputID = text
    end
})

local function playSound(soundId)
    pcall(function()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://" .. soundId
        sound.Parent = workspace
        sound:Play()
        game.Debris:AddItem(sound, 10)
    end)
end

Tab5:AddButton({
    Name = "Tocar Som",
    Description = "Toca o som inserido",
    Callback = function()
        if InputID and InputID ~= "" then
            playSound(InputID)
            game.StarterGui:SetCore("SendNotification", {
                Title = "√Åudio",
                Text = "Som tocando: " .. InputID,
                Duration = 3
            })
        end
    end
})

Tab5:AddToggle({
    Name = "Loop do Som",
    Description = "Toca em loop",
    Default = false,
    Callback = function(value)
        loopAtivo = value
        if value then
            spawn(function()
                while loopAtivo do
                    if InputID and InputID ~= "" then
                        playSound(InputID)
                    end
                    wait(3)
                end
            end)
        end
    end
})

