-- Tab5: Sistema de √Åudio (OTIMIZADO)
local Tab5 = Window:MakeTab({"Audio", "radio"})

local loopAtivo = false
local InputID = ""

Tab5:AddTextBox({
    Name = "ID do √Åudio",
    Description = "Digite o ID do som",
    Default = "",
    PlaceholderText = "Exemplo: 6832470734",
    ClearTextOnFocus = true,
    Callback = function(text)
        InputID = text
    end
})

local function playSound(soundId)
    if not soundId or soundId == "" then return end
    
    pcall(function()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://" .. soundId
        sound.Parent = workspace
        sound:Play()
        game.Debris:AddItem(sound, 10)
    end)
end

Tab5:AddButton({
    Name = "Tocar Som",
    Description = "Toca o som inserido",
    Callback = function()
        if InputID and InputID ~= "" then
            playSound(InputID)
        end
    end
})

Tab5:AddToggle({
    Name = "Loop do Som",
    Description = "Toca em loop",
    Default = false,
    Callback = function(value)
        loopAtivo = value
        if value then
            task.spawn(function()
                while loopAtivo do
                    if InputID and InputID ~= "" then
                        playSound(InputID)
                    end
                    task.wait(3)
                end
            end)
        end
    end
})

