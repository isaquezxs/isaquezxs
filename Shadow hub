    Window:EditOpenButton({
        Title = "Painel Shadow hub by doggeblox studios",
        Icon = "shield",
        CornerRadius = UDim.new(0,16),
        StrokeThickness = 2,
        Color = ColorSequence.new(
            Color3.fromHex("FF0000"), 
