-- Tab7: Sistema RGB Avan√ßado
local Tab7 = Window:MakeTab({"RGB", "rainbow"})

local Section = Tab7:AddSection({""})

-- Velocidade controlada pelo slider
local rgbSpeed = 1

Tab7:AddSlider({
    Name = "Velocidade RGB",
    Description = "Aumenta a velocidade do efeito RGB",
    Min = 1,
    Max = 5,
    Increase = 1,
    Default = 3,
    Callback = function(Value)
        rgbSpeed = Value
    end
})

-- Fun√ß√£o para criar cor RGB suave com HSV
local function getRainbowColor(speedMultiplier)
    local h = (tick() * speedMultiplier % 5) / 5
    return Color3.fromHSV(h, 1, 1)
end

-- Fun√ß√£o para disparar eventos
local function fireServer(eventName, args)
    local event = game:GetService("ReplicatedStorage"):FindFirstChild("RE")
    if event and event:FindFirstChild(eventName) then
        pcall(function()
            event[eventName]:FireServer(unpack(args))
        end)
    end
end

local Section = Tab7:AddSection({"RGB para usar em voc√™"})

-- Nome + Bio RGB juntos
local nameBioRGBActive = false
Tab7:AddToggle({
    Name = "Nome + Bio RGB",
    Default = false,
    Callback = function(state)
        nameBioRGBActive = state
        if state then
            task.spawn(function()
                while nameBioRGBActive and LocalPlayer.Character do
                    local color = getRainbowColor(rgbSpeed)
                    fireServer("1RPNam1eColo1r", { "PickingRPNameColor", color })
                    fireServer("1RPNam1eColo1r", { "PickingRPBioColor", color })
                    task.wait(0.03)
                end
            end)
        end
    end
})

-- RGB Corpo
local corpoRGBActive = false
Tab7:AddToggle({
    Name = "RGB Corpo",
    Description = "RGB no corpo",
    Default = false,
    Callback = function(state)
        corpoRGBActive = state
        if state then
            task.spawn(function()
                while corpoRGBActive do
                    local remote = game:GetService("ReplicatedStorage"):FindFirstChild("Remotes")
                    if remote and remote:FindFirstChild("ChangeBodyColor") then
                        pcall(function()
                            remote.ChangeBodyColor:FireServer({
                                BrickColor.new(getRainbowColor(rgbSpeed))
                            })
                        end)
                    end
                    task.wait(0.1)
                end
            end)
        end
    end
})

-- RGB Cabelo
local cabeloRGBActive = false
Tab7:AddToggle({
    Name = "RGB Cabelo",
    Description = "RGB no cabelo",
    Default = false,
    Callback = function(state)
        cabeloRGBActive = state
        if state then
            task.spawn(function()
                while cabeloRGBActive do
                    fireServer("1Max1y", {
                        "ChangeHairColor2",
                        getRainbowColor(rgbSpeed)
                    })
                    task.wait(0.5)
                end
            end)
        end
    end
})


local Section = Tab7:AddSection({"RGB para usar em voc√™"})

-- Nome + Bio RGB juntos
